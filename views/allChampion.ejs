<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<!DOCTYPE html>
<html>
<head>
    <style>
      body{
        background-color:#010A13;
      }
      div.logo{
        position: relative;
        top: 10px;
        left: 20px;
        color: white;
      }
      div.list{
        position: absolute;
        top: 30%;
        left: 5%;
        color: white;
        text-align: center;
      }
      div.item{
        text-align: center;
        display: inline;
        float:left;
        padding: 20px;
      }
      div.title{
        text-align: center;
        position: absolute;
        top: 10%;
        left: 50%;

      }
      img.ItemImage{
        width: 400px;
        height: 250px;
      }
      div.search{
            background-color: white;
            text-align: center;
            position: absolute;
            top:15%;
            left:70%;
            border-radius: 10px;
            border: 0px;
            width:400px;
            height:40px;
      }
      input.searchBox{
            margin-top: 10px;
            width: 300px;
            height: 25px;
            border: 2px;
            border-color: darkgrey;
      }
      button.sub {
            width:55px;
            height: 20px;
            border: 0px;
            font-weight: 400;
            color:darkgray;
      }
    </style>
    <script>
        function goHome() {
            window.location = "/";
        }
    </script>
</head>
<body>
  <div class="logo" onclick="goHome()">
    <h2>LC simulator</h2>
  </div>
  <div class="search">
    <form action="/server/findChampion" method="post">
        <input type="searchBox" type="text" name="searchWord" class="searchBox" placeholder="검색할 챔피언의 이름을 입력하세요">
        <button type="submit" class="sub">검색</button>
    </form>
  </div>
    <img style='display:inline; position:absolute; top:15%; left:50%; transform: translate(-50%, -50%);' src="/images/code/all/챔피언로고글자.jpg">
  <div class="list">
    <% result.forEach(element => { %>
        <% for(var key in element) { %>
            <div class="item">
                <img class="ItemImage" src="/images/code/all/<%= element[key] %>.jpg"><br>
                <p><%= element[key] %></p>
            </div>
            <% } %>
    <% }) %>
  </div>
    
</body>
</html>